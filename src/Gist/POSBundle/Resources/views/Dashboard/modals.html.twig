<div class="modals">
    <div aria-hidden="true" aria-labelledby="selectTransactionLabel" class="modal modal--v1 fade" id="transaction_type_modal" role="dialog" tabindex="-1">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Select Transaction</h5>
                    <!-- <button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button> -->
                </div>
                <div class="modal-body">
                    <div class="button-groups">
                        <div class="row">
                            <div class="col transaction-option transaction-option--regular" onclick="proceedToTransaction('reg')">
                                <div class="img--icon">
                                    <img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/transaction-1.png') }}" alt="">
                                </div>
                                <span>Regular Transaction</span>
                            </div>
                            <div class="col transaction-option transaction-option--perdiscount" onclick="proceedToTransaction('per')">
                                <div class="img--icon">
                                    <img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/transaction-2.png') }}" alt="">
                                </div>
                                <span>Per Item Discount</span>
                            </div>
                            <div class="col transaction-option transaction-option--bulkdiscount" onclick="proceedToTransaction('bulk')">
                                <div class="img--icon">
                                    <img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/transaction-3.png') }}" alt="">
                                </div>
                                <span>Bulk Discount</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-v1 btn-secondary" data-dismiss="modal" type="button">Cancel</button>
                </div>
            </div>
        </div>
    </div>
    <div aria-hidden="true" aria-labelledby="checkoutLabel" class="modal modal--v1 fade" id="checkout_modal" role="dialog" tabindex="-1">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <form method="post" action="">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Checkout &amp; Payment</h5>
                        <!-- <button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button> -->
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col">
                                <div class="row">
                                    <div class="col-12 col-sm-6">Amount to Pay:</div>
                                    <div class="col-12 col-sm-6 checkout-amount checkout-amount--to_pay">Php <span class="co_amt_to_pay" value="0">0.00</span></div>
                                </div>
                            </div>
                            <div class="col">
                                <div class="row">
                                    <div class="col-12 col-sm-6">Balance:</div>
                                    <div class="col-12 col-sm-6 checkout-amount checkout-amount--bal">Php <span class="co_balance" value="0">0.00</span></div>
                                </div>
                            </div>
                        </div>
                        <div class="row align-items-center">
                            <div class="col-12 col-sm-3">
                                Payment Type:
                            </div>
                            <div class="col-12 col-sm-9 payment_types">
                                <div class="row">
                                    <div class="col payment_type payment_type--cash" data-modal="#paymentCash">
                                        <a href="javascript:void(0)" class="co_cash_payment"><img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/payment-1.png') }}" alt=""></a>
                                    </div>
                                    <div class="col payment_type payment_type--cc" data-modal="#paymentCard">
                                        <a href="javascript:void(0)" class="co_cc_payment"><img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/payment-2.png') }}" alt=""></a>
                                    </div>
                                    <div class="col payment_type payment_type--check" data-modal="#paymentCheck">
                                        <a href="javascript:void(0)" class="co_check_payment"><img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/payment-3.png') }}" alt=""></a>
                                    </div>
                                    <div class="col payment_type payment_type--gc" data-modal="#paymentGift">
                                        <a href="javascript:void(0)" class="co_gc_payment"><img src="{{ asset('bundles/gisttemplate/assets/admin/pages/media/icons/payment-4.png') }}" alt=""></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-3">
                            <div class="col-12">
                                <div class="table-responsive table-v1">
                                    <table id="table--payments" class="table table-bordered" style="width:100%">
                                        <thead>
                                        <tr>
                                            <th style="">Type</th>
                                            <th style="">Amount</th>
                                            <th style=""></th>
                                        </tr>
                                        </thead>
                                        <tbody id="payments_list">
                                        {% if transaction_object.hasPayments|default(null) %}
                                            {% for p in transaction_object.getPayments %}
                                                <tr class="d-none clone-row row_payment_{{ p.getID }}">
                                                    <input type="hidden" name="control_number" class="control_number" value="{{p.getControlNumber|default('')}}">
                                                    <input type="hidden" name="account_number" class="account_number" value="{{p.getAccountNumber|default('')}}">
                                                    <input type="hidden" name="bank" class="bank" value="{{p.getBank|default('')}}">
                                                    <input type="hidden" name="payment_issued_on" class="payment_issued_on" value="{{p.getPaymentIssuedOn.getID|default('0')}}">
                                                    <input type="hidden" name="cc_terminal_opt" class="cc_terminal_opt" value="{{p.getCardTerminalOperator|default('')}}">
                                                    <input type="hidden" name="cc_interest" class="cc_interest" value="{{p.getInterest|default('')}}">
                                                    <input type="hidden" name="cc_terms" class="cc_terms" value="{{p.getCardTerms|default('')}}">
                                                    <input type="hidden" name="payee" class="payee" value="{{p.getPayee|default('')}}">
                                                    <input type="hidden" name="payor" class="payor" value="{{p.getPayor|default('')}}">
                                                    <input type="hidden" name="cc_expiry" class="cc_expiry" value="{{p.getCardExpiry|default('')}}">
                                                    <input type="hidden" name="cc_cvv" class="cc_cvv" value="{{p.getCardCvv|default('')}}">
                                                    <input type="hidden" name="payment_details_array[]" class="payment_details_array" value="null" >
                                                    <input type="hidden" name="payment_amt_float[]" class="payment_amt_float" value="{{p.amount|default('')}}" >

                                                    <input type="hidden" name="check_type[]" class="check_type" value="{{p.getCheckType|default('')}}" >
                                                    <input type="hidden" name="check_date[]" class="check_date" value="{{p.getCheckDate|default('')}}" >

                                                    <td class="item_type"><input type="text" style="font-size: 12px !important;" name="payment_type[]" value="{{p.getType|default('')}}" readonly="true" class="form-control payment_type"></td>
                                                    <td class="item_amount"><input type="text" style="font-size: 10px !important;" name="payment_amt[]" class="form-control payment_amt" readonly="true" value="{{p.amount|number_format(2)|default('')}}"></td>
                                                    <td ><a href="javascript:void(0)" class="btn btn-xs default red x_remove_payment_row"><i class="fa fa-times" aria-hidden="true"></i></a></td>
                                                </tr>
                                            {% endfor %}
                                        {% else %}
                                            <tr class="init_row_payment">
                                                <td colspan="3"><p>No payments added</p></td>
                                            </tr>
                                        {% endif %}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-v1 btn-small btn-success finish_btn" data-dismiss="modal" type="button">Finish</button>
                        <button class="btn btn-v1 btn-small btn-secondary" data-dismiss="modal" type="button">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
