                            {{ pos_form_tools.group_text('Less Amount', 'discount_amount', transaction_object.getSelectedBulkDiscountAmount|default(0), 3, 5, false) }}
                            {{ pos_form_tools.group_text('Less %', 'discount_pct', transaction_object.getSelectedBulkDiscountAmount|default(0), 3, 5, false) }}
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-v1 btn-small btn-success apply_bulk" data-dismiss="modal" type="button">Apply</button>
                        <button class="btn btn-v1 btn-small btn-secondary" data-dismiss="modal" type="button">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
